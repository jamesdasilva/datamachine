  "compilerOptions": {
    "outDir": "./",
    "sourceMap": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "noLib": false,
    "target": "es5",
    "allowJs": true,
    "strict": false
  },
  "exclude": [
    "node_modules"
  ]
}